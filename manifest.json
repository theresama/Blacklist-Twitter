{
  "name": "Twitter Blacklist",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Blacklist twitter posts",

  "content_scripts": [
  {
    "matches": ["*://twitter.com/*", "*://www.twitter.com/*"],
    "js": ["jquery.js", "blacklist.js"]
  }
  ],

  "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  }
}